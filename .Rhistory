is_numberic <- function(x){
if(is.numeric(x)){
if (x>0){
return(1);
}
if (x==0){
warning("Input is zero!")
return(NaN)
} else {
return(-1)
}
} else{
stop("Input number be a number!")
}
}
is_numberic(-23)
typeof(-23)
is_numberic("a")
is_numberic(-23)
is_numberic(0)
is_numberic(10.5)
is_numberic(TRUE)
